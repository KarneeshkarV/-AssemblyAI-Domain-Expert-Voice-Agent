# System Configuration
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Optional: Local Ollama Configuration
OLLAMA_HOST=localhost
OLLAMA_EMBEDDING_MODEL=nomic-embed-text:latest